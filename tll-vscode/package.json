{
  "name": "tll-vscode",
  "displayName": "tll-vscode",
  "description": "VSCode support for TLL",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "tll",
      "aliases": ["TLL", "tll"],
      "extensions": [".tll"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "tll",
      "scopeName": "source.tll",
      "path": "./syntaxes/tll.tmLanguage.json"
    }]
  }
}
